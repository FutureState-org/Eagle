{
  "allowActionHistory": true,
  "allowRoleRequest": true,
  "allowNotification": true,
  "maxExpiryDate": 365,
  "optimizedWorkFlow": {
    "allow": true,
    "conditions": {
      "fit": [
        "*"
      ],
      "notFit": []
    }
  },
  "workFlowTable": [
    {
      "conditions": {
        "fit": [
          "*"
        ],
        "notFit": []
      },
      "workFlow": [
        "Draft",
        "InReview",
        "Reviewed",
        "Processing",
        "Live"
      ]
    }
  ],
  "ownerDetails": [
    {
      "status": [
        "Draft",
        "Live"
      ],
      "owner": "creatorContacts",
      "name": "Creator",
      "relatedActions": [
        "submitted",
        "retrieved",
        "restored",
        "pulledBack"
      ]
    },
    {
      "status": [
        "MarkedForDeletion"
      ],
      "actionName": "Marked for deletion",
      "owner": "creatorContacts",
      "name": "Creator",
      "relatedActions": [
        "markedForDeletion"
      ]
    },
    {
      "status": [
        "Unpublished"
      ],
      "actionName": "Unpublished",
      "owner": "creatorContacts",
      "name": "Creator",
      "relatedActions": [
        "unpublished"
      ]
    },
    {
      "status": [
        "Deleted"
      ],
      "actionName": "Deleted",
      "owner": "creatorContacts",
      "name": "Creator",
      "relatedActions": [
        "deleted"
      ]
    },
    {
      "status": [
        "InReview"
      ],
      "actionName": "Review",
      "owner": "trackContacts",
      "name": "Reviewer",
      "relatedActions": [
        "reviewerApproved",
        "reviewerRejected"
      ]
    },
    {
      "status": [
        "Reviewed"
      ],
      "actionName": "Publish",
      "owner": "publisherDetails",
      "name": "Publisher",
      "relatedActions": [
        "publisherApproved",
        "publisherRejected"
      ]
    },
    {
      "status": [
        "QualityReview"
      ],
      "actionName": "Quality Review",
      "relatedActions": [
        "qualityApproved",
        "qualityRejected"
      ]
    },
    {
      "status": [
        "Expired"
      ],
      "relatedActions": [
        "expired"
      ]
    },
    {
      "status": [
        "Processing"
      ],
      "relatedActions": []
    }
  ],
  "permissionDetails": [
    {
      "role": "author",
      "editContent": {
        "conditions": {
          "fit": [
            {
              "status": [
                "Draft",
                "Live"
              ]
            }
          ],
          "notFit": []
        }
      },
      "editMeta": {
        "conditions": {
          "fit": [
            {
              "status": [
                "Draft",
                "Live"
              ]
            }
          ],
          "notFit": []
        }
      }
    },
    {
      "role": "reviewer",
      "editMeta": {
        "enableByDefault": false,
        "conditions": {
          "fit": [
            {
              "status": [
                "Draft",
                "Live"
              ]
            }
          ],
          "notFit": []
        }
      }
    },
    {
      "role": "publisher",
      "editMeta": {
        "enableByDefault": false,
        "conditions": {
          "fit": [
            {
              "status": [
                "Draft",
                "Live"
              ]
            }
          ],
          "notFit": []
        }
      }
    }
  ],
  "menus": {
    "widgetType": "menus",
    "widgetSubType": "leftMenu",
    "widgetData": [
      {
        "name": "All CBPs",
        "key": "cbp",
        "fragment": false,
        "render": true,
        "badges": {
          "enabled": false,
          "uri": ""
        },
        "enabled": true,
        "routerLink": "/author/home"
      },
      {
        "name": "Users",
        "key": "users",
        "fragment": false,
        "render": true,
        "badges": {
          "enabled": false,
          "uri": ""
        },
        "enabled": true,
        "routerLink": "/app/person-profile"
      },
      {
        "name": "Competencies",
        "key": "competencies",
        "fragment": false,
        "render": true,
        "badges": {
          "enabled": false,
          "uri": ""
        },
        "enabled": true,
        "routerLink": "/app/notifications"
      },
      {
        "name": "Policies",
        "key": "policies",
        "fragment": false,
        "render": true,
        "badges": {
          "enabled": false,
          "uri": ""
        },
        "enabled": true,
        "routerLink": "/app/notifications"
      },
      {
        "name": "About",
        "key": "about",
        "fragment": false,
        "render": true,
        "badges": {
          "enabled": false,
          "uri": ""
        },
        "enabled": true,
        "routerLink": "/app/home/about"
      }
    ]
  }
